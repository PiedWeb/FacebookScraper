{
    "name": "piedweb/facebook-scraper",
    "description": "{ Harvest Data | Scrap } from Scrape Facebook public pages without an API key",
    "keywords": [
        "piedweb",
        "FacebookScraper"
    ],
    "homepage": "https://dev.piedweb.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin",
            "email": "contact@piedweb.com",
            "homepage": "https://dev.piedweb.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "league/html-to-markdown": "^4.10",
        "piedweb/curl": "^0.0.18",
        "symfony/css-selector": "^5.2",
        "symfony/dom-crawler": "^5.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "PiedWeb\\FacebookScraper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PiedWeb\\FacebookScraper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "prefer-stable": true
}
